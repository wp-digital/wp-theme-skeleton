{
  "plugins": ["prettier", "jest"],
  "extends": ["airbnb", "prettier", "prettier/react"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true,
    "worker": true,
    "amd": true,
    "jquery": true,
    "serviceworker": true,
    "jest/globals": true
  },
  "settings": {
    "import/core-modules": ["jquery"]
  },
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": 0,
    "jsx-a11y/label-has-associated-control": ["error", {
      "labelComponents": [],
      "labelAttributes": [],
      "controlComponents": [],
      "assert": "either",
      "depth": 25
    }],
    "react/jsx-props-no-spreading": 0,
    "react/no-did-update-set-state": 0,
    "react/no-danger": 0
  }
}
